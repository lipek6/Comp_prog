#include<bits/stdc++.h>
using namespace std;

//The color of cell (x,y) is black if 0≤x≤X−1 and 0≤y≤Y−1, and white otherwise.
int main (void)
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int N, X, Y; cin >> N >> X >> Y;


    for(int i = 0; i < N; i++)
    {
        char c;
        int a , b;
        cin >> c >> a >> b;
        
        int old_cell_color;
        if(c == 'X')
        {  
            if(X < a)   // it becomes the color of cell (x,y+Bi​) before the great storm;
            {
                if(X-1 && Y-1)
                old_cell_color = X, Y + b

            }
            else        // it becomes the color of cell (x,y−Bi​) before the great storm.
            {

            }

        }
        else
        {
            if(Y < a)   // it becomes the color of cell (x+Bi​,y) before the great storm;
            {

            }
            else        // it becomes the color of cell (x−Bi​,y) before the great storm.
            {

            }
        }



    }

}